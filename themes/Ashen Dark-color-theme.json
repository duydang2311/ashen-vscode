{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ashen Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.defaultLibrary": "#4A8B8B",
    "parameter.declaration": "#d5d5d5"
  },
  "colors": {
    "editor.background": "#121212",
    "editor.foreground": "#b4b4b4",
    "editorLineNumber.foreground": "#323232",
    "editorLineNumber.activeForeground": "#737373",
    "editor.selectionBackground": "#322119",
    "editor.lineHighlightBackground": "#191919",
    "editorIndentGuide.background1": "#212121",
    "editorIndentGuide.activeBackground1": "#737373",
    "editorWhitespace.foreground": "#535353",
    "editorCursor.background": "#121212",
    "editorCursor.foreground": "#6D3B22",
    "button.background": "#B14242",
    "button.foreground": "#121212",
    "dropdown.background": "#212121",
    "dropdown.listBackground": "#212121",
    "focusBorder": "#535353",
    "sideBarSectionHeader.background": "#191919",

    "statusBar.background": "#212121",
    "statusBar.foreground": "#b4b4b4",
    "statusBar.debuggingBackground": "#212121",
    "statusBar.debuggingForeground": "#b4b4b4",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.noFolderForeground": "#b4b4b4",

    "sideBar.background": "#121212",
    "sideBar.foreground": "#b4b4b4",
    "sideBar.border": "#212121",

    "activityBar.background": "#121212",
    "activityBar.foreground": "#b4b4b4",

    "panel.background": "#121212",
    "panel.border": "#212121",
    "panelTitle.border": "#212121",
    "sideBarSectionHeader.border": "#212121",
    "quickInput.background": "#191919",
    "quickInput.foreground": "#b4b4b4",
    "quickInputList.focusBackground": "#212121",
    "quickInputList.focusForeground": "#e5e5e5",
    "quickInputList.focusIconForeground": "#e5e5e5",
    "list.highlightForeground": "#B14242",
    "list.focusHighlightForeground": "#d75050",
    "input.background": "#191919",
    "input.border": "#212121",
    "editorGroupHeader.border": "#212121",

    "titleBar.activeBackground": "#121212",
    "titleBar.activeForeground": "#b4b4b4",
    "titleBar.inactiveBackground": "#121212",
    "titleBar.border": "#212121",
    "titleBar.inactiveForeground": "#b4b4b4",
    "editorGroup.border": "#323232",

    "terminal.background": "#121212",
    "terminal.foreground": "#b4b4b4",

    // Diagnostics
    "editorError.foreground": "#C53030",
    "editorWarning.foreground": "#E5A72A",
    "editorInfo.foreground": "#a7a7a7",
    "editorHint.foreground": "#949494",

    "editorSuggestWidget.background": "#212121",
    "editorSuggestWidget.border": "#323232",
    "editorSuggestWidget.selectedBackground": "#323232",
    "editorSuggestWidget.foreground": "#b4b4b4",
    "editorSuggestWidget.selectedForeground": "#e5e5e5",
    "editorSuggestWidget.highlightForeground": "#B14242",
    "editorSuggestWidget.focusHighlightForeground": "#d75050",

    // Terminal ANSI
    "terminal.ansiBlack": "#121212",
    "terminal.ansiRed": "#B14242",
    "terminal.ansiGreen": "#D87C4A",
    "terminal.ansiYellow": "#E49A44",
    "terminal.ansiBlue": "#4A8B8B",
    "terminal.ansiMagenta": "#a7a7a7",
    "terminal.ansiCyan": "#b4b4b4",
    "terminal.ansiWhite": "#d5d5d5",
    "terminal.ansiBrightBlack": "#949494",
    "terminal.ansiBrightRed": "#B14242",
    "terminal.ansiBrightGreen": "#D87C4A",
    "terminal.ansiBrightYellow": "#E49A44",
    "terminal.ansiBrightBlue": "#4A8B8B",
    "terminal.ansiBrightMagenta": "#a7a7a7",
    "terminal.ansiBrightCyan": "#b4b4b4",
    "terminal.ansiBrightWhite": "#e5e5e5",

    "textLink.foreground": "#B14242",
    "textLink.activeForeground": "#d75050"
  },
  "tokenColors": [
    // orange blaze
    {
      "scope": [
        "constant",
        "keyword.control.as",
        "keyword.operator.expression",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#C4693D"
      }
    },

    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#4A8B8B"
      }
    },

    // red glowing
    {
      "scope": [
        "constant.character",
        "punctuation.definition.string",
        "string"
      ],
      "settings": {
        "foreground": "#DF6464"
      }
    },

    // red ember
    {
      "scope": [
        "keyword.control.import",
        "keyword.operator.new",
        "keyword.control.from",
        "keyword.control.export",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.trycatch",
        "keyword.control.loop",
        "keyword.control.switch",
        "storage.modifier",
        "keyword.other.directive",
        "storage.type",
        "variable.language",
        "storage.type.function",
        "meta.special",
        "keyword.other.svelte",
        "meta.attribute.rust",
        "keyword.other.rust",
        "keyword.other.crate.rust",
        "keyword.other.fn.rust",
        "entity.name.type.lifetime.rust",
        "punctuation.definition.lifetime.rust",
        "keyword.operator.borrow.and.rust",
        "keyword.control.rust",
        "entity.name.function.macro.rust",
        "entity.name.function.macro.rules.rust",
      ],
      "settings": {
        "foreground": "#B14242"
      }
    },

    // blue
    {
      "scope": [
        "keyword.other",
        "storage.type.numeric",
        "entity.name.type",
        "keyword.type",
        "constant.language",
        "support.type.builtin",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#4A8B8B"
      }
    },

    // brown
    {
      "scope": ["meta.type"],
      "settings": {
        "foreground": "#89492a"
      }
    },

    // g_5
    {
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#949494",
        "fontStyle": "italic"
      }
    },

    // text
    {
      "scope": [
        "variable",
        "entity.name.function",
        "entity.name.type.namespace",
        "support.type",
        "punctuation.definition.variable",
        "meta",
        "entity.name.type.svelte",
        "keyword.other.animation.svelte",
        "entity.other.attribute-name.svelte"
      ],
      "settings": {
        "foreground": "#b4b4b4"
      }
    },

    // orange glow
    {
      "scope": [
        "punctuation.definition",
        "string.regexp",
        "entity.name.tag",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "meta.tag.start",
        "meta.tag.end",
        "keyword.operator.comparison",
        "keyword.operator.relational",
        "storage.type.function.arrow",
        "entity.name.module.rust",
        "keyword.operator.arrow.skinny.rust",
        "keyword.operator.key-value.rust",
        "punctuation.semi.rust",
        "punctuation.comma.rust",
        "entity.name.namespace.rust",
        "keyword.operator.arrow.fat.rust",
        "meta.import.rust"
      ],
      "settings": {
        "foreground": "#d87c4a"
      }
    },

    // orange smolder
    {
      "scope": [
        "punctuation.terminator",
        "punctuation.accessor",
        "punctuation.separator",
        "keyword.operator.type",
        "keyword.operator.namespace.rust",
        "keyword.operator.access.dot.rust"
      ],
      "settings": {
        "foreground": "#E49A44"
      }
    },

    // orange golden
    {
      "scope": ["punctuation.definition.template-expression",
    "punctuation.definition.interpolation.rust"],
      "settings": {
        "foreground": "#E5A72A"
      }
    },

    // g_6
    {
      "scope": [
        "punctuation.curlybrace",
        "punctuation.parenthesis",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.parameters",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.array",
        "meta.brace",
        "comment",
        "punctuation.definition.comment",
        "punctuation.definition.attribute.rust",
        "punctuation.brackets.attribute.rust",
        "punctuation.brackets.round.rust",
        "punctuation.brackets.curly.rust",
        "punctuation.brackets.angle.rust"
      ],
      "settings": {
        "foreground": "#737373"
      }
    },

    // g_2
    {
      "scope": [
        "entity.name.variable.parameter",
        "punctuation.separator.dictionary.key-value",
        "punctuation.definition.tag",
        "punctuation.definition.keyword",
        "variable.language.svelte",
        "punctuation.section.embedded.begin.svelte",
        "punctuation.section.embedded.end.svelte",
        "variable.other.property.ts",
      ],
      "settings": {
        "foreground": "#d5d5d5"
      }
    }
  ]
}

